<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper
	PUBLIC "-//mybatis.org//DTD Mapper 3.0//EX"
	"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
	<mapper namespace="net.daum.byb.service.MemberDao">
		
		<insert id="insertRow" parameterType="hashMap"  >
			INSERT INTO member
				(
				    email,
				    nickname,
				    password,
				    seq
				)
				VALUES
				(
				 	#{email},
				    #{nickname},			 	
				    #{password},
				    1
				)
		</insert>
	
	
	
	
	</mapper>